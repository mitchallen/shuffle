{
  "name": "@mitchallen/shuffle",
  "version": "0.1.0",
  "description": "Uses Fisher-Yates to shuffle an array",
  "main": "dist/shuffle.js",
  "scripts": {
    "//": "To use: npm start (useful if not a module)",
    "start": "node dist/shuffle.js",
    "test": "grunt && mocha --recursive --timeout 20000",
    "watch": "grunt monitor",
    "test-nodemon": "nodemon /usr/local/bin/mocha --recursive --timeout 5000",
    "test-debug": "nodemon /usr/local/bin/mocha -g '@DEBUG' --recursive --timeout 20000",
    "test-jenkins": "grunt && /usr/local/bin/mocha --recursive --timeout 20000 -R xunit test/ > test-reports.xml"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/mitchallen/shuffle.git"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mitchallen/shuffle.git"
  },
  "author": "Mitch Allen <npm@mitchallen.com> (http://mitchallen.com)",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-bump": "^0.8.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-shell": "^1.3.0",
    "should": "^10.0.0",
    "supertest": "^2.0.0"
  }
}
